<?xml version="1.0" encoding="utf-8"?>
<manifest package="despotoski.nikola.github.com.bottomnavigationlayout.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="despotoski.nikola.github.com.sample.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeSimple">
        <activity android:name="despotoski.nikola.github.com.sample.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="despotoski.nikola.github.com.sample.MainActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name="despotoski.nikola.github.com.sample.DemoActivity">
        </activity>
    </application>

</manifest>